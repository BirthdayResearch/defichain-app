{
  "compilerOptions": {
    "outDir": "./electron-app/build/",
    "module": "commonjs",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    },
    "resolveJsonModule": true
  },
  "include": ["electron-app/**/*"],
  "exclude": ["electron-app/build/"]
}
